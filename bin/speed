speedtest=$(speedtest)

downloadSpeed=$(echo $speedtest | grep -Eo "Download: \d+.\d+ Mbit/s") 
uploadSpeed=$(echo $speedtest | grep -Eo "Upload: \d+.\d+ Mbit/s")

error=$(echo $speedtest | grep -Eo "Cannot retrieve speedtest configuration")

HOME="/Users/michael"

echo $(date +"%d.%m.%Y %H:%M:%S") >> $HOME/Desktop/speetest.txt
echo $downloadSpeed >> $HOME/Desktop/speetest.txt
echo $uploadSpeed >> $HOME/Desktop/speetest.txt
echo $error >> $HOME/Desktop/speetest.txt
echo "------------------" >> $HOME/Desktop/speetest.txt
echo "" >> $HOME/Desktop/speetest.txt

echo "Command successful"
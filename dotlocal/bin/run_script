#!/usr/bin/env bash

commands=(
  bookmark-add
  bookmarks
  copy-spotify-url
  "clipctl toggle"
  dict
  dmenuhandler
  emoji
  mkscript
  nothis
  "profile work"
  "profile home"
  restart
  sessions
  testfile
  watch-later.sh
  wifi-menu.sh
)

command=$(printf "%s\n" "${commands[@]}" | rofi -dmenu -l 7 -p "Command to run")

$command

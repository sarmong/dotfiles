#!/usr/bin/env bash

commands=(
  bookmark-add
  bookmarks
  copy-spotify-url
  "clipctl toggle"
  dict
  open
  "open --clip"
  "open --ask"
  emoji
  fcp
  "fcp --drag"
  mkscript
  mounter
  nothis
  "profile work"
  "profile home"
  restart
  search
  sessions
  testfile
  watch-later.sh
  wifi-menu.sh
)

